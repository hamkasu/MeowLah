{
  "name": "meowlah-backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "prisma generate && tsc",
    "start": "npx prisma db push --accept-data-loss && node dist/server.js",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "seed": "ts-node prisma/seed.ts",
    "lint": "eslint src/"
  },
  "dependencies": {
    "express": "^4.19.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.0",
    "@prisma/client": "^5.12.0",
    "prisma": "^5.12.0",
    "ioredis": "^5.3.2",
    "@aws-sdk/client-s3": "^3.550.0",
    "@aws-sdk/s3-request-presigner": "^3.550.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.3",
    "web-push": "^3.6.7",
    "slugify": "^1.6.6",
    "nanoid": "^3.3.7",
    "uuid": "^9.0.1",
    "puppeteer": "^22.6.0",
    "dotenv": "^16.4.0",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "typescript": "^5.4.0",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "@types/web-push": "^3.6.3",
    "@types/uuid": "^9.0.8",
    "@types/node": "^20.12.0",
    "eslint": "^8.57.0"
  }
}
